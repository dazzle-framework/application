{
    "name": "dazzlesoftware/application",
    "type": "library",
    "description": "Dazzle Framework Application Package",
    "keywords": ["dazzle", "framework", "application"],
    "homepage": "https://github.com/dazzle-framework/application",
    "license": "GPL-3.0-or-later",
    "require": {
        "php": "^7.3.0",
        "dazzlesoftware/event": "^1.0",
        "dazzlesoftware/registry": "^1.0",
        "laminas/laminas-diactoros": "^2.2.2",
        "psr/log": "^1.0",
        "psr/http-message": "^1.0"
    },
    "require-dev": {
        "dazzlesoftware/dazzle-coding-standard": "^1.0",
        "dazzlesoftware/controller": "^1.0",
        "dazzlesoftware/di": "^1.0",
        "dazzlesoftware/input": "^1.0",
        "dazzlesoftware/router": "^1.0",
        "dazzlesoftware/session": "^1.0",
        "dazzlesoftware/test": "^1.0",
        "dazzlesoftware/uri": "^1.0",
        "phpunit/phpunit": "^9.0",
        "symfony/phpunit-bridge": "^3.4|^4.4|^5.0"
    },
    "suggest": {
        "dazzlesoftware/controller": "^1.0 To support resolving ControllerInterface objects in ControllerResolverInterface, install dazzlesoftware/controller",
        "dazzlesoftware/input": "^1.0 To use WebApplicationInterface, install dazzlesoftware/input",
        "dazzlesoftware/router": "^1.0 To use WebApplication or ControllerResolverInterface implementations, install dazzlesoftware/router",
        "dazzlesoftware/session": "^1.0 To use SessionAwareWebApplicationInterface, install dazzlesoftware/session",
        "dazzlesoftware/uri": "^1.0 To use AbstractWebApplication, install dazzlesoftware/uri",
        "psr/container": "^1.0 To use the ContainerControllerResolver, install any PSR-11 compatible container"
    },
    "autoload": {
        "psr-4": {
            "Dazzle\\Application\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Dazzle\\Application\\UnitTests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev"
        }
    }
}
